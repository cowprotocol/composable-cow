      "@safe-global/safe-ethers-lib": "^1.9.3",
      "commander": "^10.0.1",
      "dotenv": "^16.0.3",
      "ethers": "^5.7.2",
      "node-fetch": "2",
      "typescript": "^5.0.4"
    },
